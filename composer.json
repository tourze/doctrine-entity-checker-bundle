{
  "name": "tourze/doctrine-entity-checker-bundle",
  "description": "",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/orm": "^2.17 || ^3.0",
    "doctrine/doctrine-bundle": "^2.13",
    "symfony/framework-bundle": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "psr/container": "^1.1|^2.0",
    "yiisoft/strings": "^2.1",
    "yiisoft/json": "^1.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0",
    "symfony/phpunit-bridge": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\DoctrineEntityCheckerBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\DoctrineEntityCheckerBundle\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1",
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html=build/coverage"
  }
}