{
  "name": "tourze/doctrine-entity-checker-bundle",
  "description": "A Symfony Bundle that provides Doctrine entity checking and processing capabilities with custom checkers, ID generators, and SQL formatting tools.",
  "license": "MIT",
  "require": {
    "doctrine/collections": "^2.3",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^4.1",
    "psr/container": "^1.1|^2.0",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/symfony-dependency-service-loader": "1.*",
    "yiisoft/json": "^1.0",
    "yiisoft/strings": "^2.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/phpunit-bridge": "^7.3",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "0.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\DoctrineEntityCheckerBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\DoctrineEntityCheckerBundle\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1",
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html=build/coverage"
  }
}